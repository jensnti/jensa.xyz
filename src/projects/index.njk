{% extends "layouts/base.njk" %}

{% block content %}
<main id="main-content" tabindex="-1">
    <section class="content container flow">
        <p class="content__lead">Länkar och eventuell info om projekt jag har/är delaktig i.</p>
        {% set projects = collections.projects %}
        {% if projects.length %}
        <ul class="project-list">
            {% for project in projects %}
                <li><a href="{{ project.url }}">
                    {{ project.data.title }}
                </a>
            </li>
            {% endfor %}
        </ul>
        {% endif %}
    </section>
</main>
{% endblock %}
